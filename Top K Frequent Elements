class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        freq_map = {}
        for n in nums:
            freq_map[n] = 1 + freq_map.get(n, 0)
            
        freqs = []
        for num, freq in freq_map.items():
            freqs.append([num, freq])
            
        freqs.sort(key=lambda x: x[1], reverse=True)
        
        result = []
        for i in range(k):
            result.append(freqs[i][0])
            
        return result
